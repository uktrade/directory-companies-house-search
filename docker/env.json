{
  "host_env_var_prefix": "DIRECTORY_CH_SEARCH",
  "file_path": "./docker/.env",
  "env_vars": {
    "required": [
      "PORT",
      "SIGNATURE_SECRET",
      "SECRET_KEY",
      "DATABASE_URL",
      "SESSION_COOKIE_DOMAIN",
      "GECKO_API_KEY",
      "REDIS_URL",
      "ELASTICSEARCH_ENDPOINT",
      "ELASTICSEARCH_AWS_ACCESS_KEY_ID",
      "ELASTICSEARCH_AWS_SECRET_ACCESS_KEY",
      "HEALTH_CHECK_TOKEN"
    ],
    "optional": [
      "DEBUG",
      "CSRF_COOKIE_SECURE",
      "SESSION_COOKIE_SECURE",
      "GECKO_API_PASS",
      "ELASTICSEARCH_PORT",
      "ELASTICSEARCH_USE_SSL",
      "ELASTICSEARCH_VERIFY_CERTS",
      "ELASTICSEARCH_COMPANY_INDEX_ALIAS",
      "ELASTICSEARCH_CHUNK_SIZE",
      "ELASTICSEARCH_TIMEOUT_SECONDS"
    ]
  }
}
